<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="paperformat_custom" model="report.paperformat">
            <field name="name">Standar custom</field>
            <field name="default" eval="True"/>
            <field name="format">A4</field>
            <field name="page_width">0</field>
            <field name="page_width">0</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">20</field>
            <field name="margin_right">5</field>
            <field name="margin_bottom">10</field>
            <field name="margin_left">5</field>
            <field name="header_line" eval="False"/>
            <field name="header_spacing">10</field>
            <field name="dpi">90</field>
        </record>

        <record id="mail_activity_data_call_phone" model="mail.activity.type">
            <field name="name">Llamada telefónica</field>
            <field name="icon">fa-phone</field>
            <field name="sequence">3</field>
        </record>

        <record id="mail_activity_data_appointment_at_the_headquarters" model="mail.activity.type">
            <field name="name">Cita en sede</field>
            <field name="icon">fa-envelope</field>
            <field name="sequence">3</field>
        </record>

        <record id="mail_activity_data_coordination_with_other_entities" model="mail.activity.type">
            <field name="name">Coordinación con otras entidades</field>
            <field name="icon">fa-users</field>
            <field name="sequence">3</field>
        </record>

        <record id="mail_activity_data_house_call" model="mail.activity.type">
            <field name="name">Visita a domicilio</field>
            <field name="icon">fa-house</field>
            <field name="sequence">3</field>
        </record>

        <record id="mail_activity_data_derivate_case" model="mail.activity.type">
            <field name="name">Derivar el caso</field>
            <field name="icon">fa-archive</field>
            <field name="sequence">3</field>
        </record>

        <record id="mail_activity_data_closing_the_case" model="mail.activity.type">
            <field name="name">Cierre de caso</field>
            <field name="icon">fa-lock</field>
            <field name="sequence">3</field>
        </record>

        <record id="mail_activity_data_development_of_pai" model="mail.activity.type">
            <field name="name">Desarrollo de pai</field>
            <field name="icon">fa-cogs</field>
            <field name="sequence">3</field>
        </record>

        <record id="mail_activity_data_follow_up_of_pai" model="mail.activity.type">
            <field name="name">Seguimiento de pai</field>
            <field name="icon">fa-envelope</field>
            <field name="sequence">3</field>
        </record>
    </data>
</odoo>
